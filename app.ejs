<!DOCTYPE html> 
<html lang="en"> 
  
<head> 
    <meta charset="UTF-8"> 
    <meta name="viewport" content="width=device-width, initial-scale=1.0"> 
    <title>Image Uploading</title> 
</head> 
  
<body> 
    <div> 
        <h3>New Whiteboard</h3> 
        <button onclick="newWhiteboard()">New</button>
    </div> 
    <hr>
    <h1>Uploaded Whiteboards</h1> 
    <div> 
        <% items.forEach(function(image) { %> 
        <div> 
            <div> 
                <div> 
                    <h5><%= image.title %></h5> 
                </div> 
                <img src="data:image/<%=image.img.contentType%>;base64, 
                     <%=image.img.data.toString('base64')%>" width="200px" height="100px" onclick="imgHandler('<%= image.title %>')">  
            </div> 
            <hr>
        </div> 
        <% }) %> 
    </div> 
</body> 

<script>
    function imgHandler(title)
    {
        window.location.href = "/whiteboard/board/" + title;
    }

    function newWhiteboard()
    {
        window.location.href = "/whiteboard/board/";
    }
</script>
  
</html> 